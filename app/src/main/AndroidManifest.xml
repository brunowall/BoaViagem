    package="br.ufc.quixada.boaviagem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".br.ufc.quixada.boaviagem.views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".br.ufc.quixada.boaviagem.views.TelaInicial" />
        <activity android:name=".br.ufc.quixada.boaviagem.views.AddViagem" />
        <activity android:name=".br.ufc.quixada.boaviagem.views.NovoGastoActivity" />
        <activity android:name=".br.ufc.quixada.boaviagem.views.VerViagens" />
        <activity android:name=".br.ufc.quixada.boaviagem.views.ConfiguracoesActivity" />
    </application>

</manifest>